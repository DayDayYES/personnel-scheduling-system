e-column prop=\"no\" label=\"账号\" width=\"180\">\r\n      </el-table-column>\r\n\r\n      <el-table-column prop=\"name\" label=\"姓名\" width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"age\" label=\"年龄\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"sex\" label=\"性别\" width=\"80\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n            :type=\"scope.row.sex == '1' ? 'primary' : 'success'\"\r\n            disable-transitions>{{scope.row.sex == '1' ? '男' : '女'}}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"电话\" width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"roleId\" label=\"角色\" width=\"120\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n              :type=\"scope.row.roleId == '0' ? 'danger' : (scope.row.roleId == '1' ? 'primary' : 'success')\"\r\n              disable-transitions>{{scope.row.roleId == '0' ? '超级管理员' : (scope.row.roleId == '1' ? '管理员' : '用户')}}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"operate\" label=\"操作\">\r\n        <el-button size=\"small\" type=\"success\">编辑</el-button>\r\n        <el-button size=\"small\" type=\"danger\">删除</el-button>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pageNum\"\r\n        :page-sizes=\"[5, 10, 20, 30]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n    </el-pagination>\r\n\r\n    <el-dialog\r\n        title=\"提示\"\r\n        :visible.sync=\"centerDialogVisible\"\r\n        width=\"30%\"\r\n        center>\r\n\r\n      <el-form ref=\"form\" :model=\"form\" label-width=\"80px\">\r\n\r\n        <el-form-item prop=\"name\" label=\"姓名\">\r\n          <el-col :span=\"20\">\r\n            <el-input v-model=\"form.name\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n        <el-form-item label=\"账号\">\r\n          <el-col :span=\"20\">\r\n            <el-input v-model=\"form.no\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\">\r\n          <el-col :span=\"20\">\r\n            <el-input v-model=\"form.password\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-radio-group v-model=\"form.sex\">\r\n            <el-radio label=\"1\">男</el-radio>\r\n            <el-radio label=\"0\">女</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item label=\"年龄\">\r\n          <el-col :span=\"20\">\r\n            <el-input v-model=\"form.age\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n        <el-form-item label=\"电话\">\r\n          <el-col :span=\"20\">\r\n            <el-input v-model=\"form.phone\"></el-input>\r\n          </el-col>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"centerDialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n  </span>\r\n    </el-dialog>\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"MyMain\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      pageSize: 10,\r\n      pageNum: 1,\r\n      total: 0,\r\n      name: '',\r\n      sex: '',\r\n      sexs: [\r\n        {value: '1', label: '男'},\r\n        {value: '0', label: '女'}\r\n      ],\r\n      centerDialogVisible: false,\r\n      form: {\r\n        no: '',\r\n        password: '',\r\n        name: '',\r\n        age: '',\r\n        sex: '0',\r\n        phone: '',\r\n        roleId: '1',\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    resetForm(formName) {\r\n      this.$refs.form.resetFields();\r\n    },\r\n    add() {\r\n      this.centerDialogVisible = true\r\n      this.$nextTick(() => {\r\n        this.resetForm('form')\r\n      })\r\n      //this.$router.push('/add')\r\n    },\r\n    save() {\r\n      this.$axios.post(this.$httpUrl + '/user/save', this.form).then(res => res.data).then(res => {\r\n        console.log(res)\r\n        if (res.code == 200) {\r\n          // this.tableData = res.data\r\n          // this.total = res.total\r\n          this.$message({\r\n            message: '保存成功!',\r\n            type: 'success'\r\n          });\r\n          this.centerDialogVisible = false\r\n          this.loadPost()\r\n\r\n        } else {\r\n          this.$message({\r\n            message: '保存失败!',\r\n            type: 'error'\r\n          });\r\n        }\r\n      });\r\n    },\r\n    loadGet() {\r\n      this.$axios.get(this.$httpUrl + '/user/list').then(res => res.data).then(res => {\r\n        console.log(res);\r\n      });\r\n    },\r\n    resetParam() {\r\n      this.name = ''\r\n      this.sex = ''\r\n    },\r\n    loadPost() {\r\n      this.$axios.post(this.$httpUrl + '/user/listPageC1', {\r\n        pageNum: this.pageNum,\r\n        pageSize: this.pageSize,\r\n        param: {\r\n          name: this.name,\r\n          sex: this.sex\r\n        }\r\n      }).then(res => res.data).then(res => {\r\n        console.log(res)\r\n        if (res.code == 200) {\r\n          this.tableData = res.data\r\n          this.total = res.total\r\n        } else {\r\n          alert('获取数据失败')\r\n        }\r\n      });\r\n    },\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`);\r\n      this.pageNum = 1\r\n      this.pageSize = val\r\n      this.loadPost()\r\n    },\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.pageNum = val\r\n      this.loadPost()\r\n    }\r\n  },\r\n  beforeMount() {\r\n    //this.loadGet(\r\n    this.loadPost()\r\n  }\r\n};\r\n\r\n</script>"],"mappings":"AAoHA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,QAAA;MACAC,OAAA;MACAC,KAAA;MACAL,IAAA;MACAM,GAAA;MACAC,IAAA,GACA;QAAAC,KAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,KAAA;MAAA,EACA;MACAC,mBAAA;MACAC,IAAA;QACAC,EAAA;QACAC,QAAA;QACAb,IAAA;QACAc,GAAA;QACAR,GAAA;QACAS,KAAA;QACAC,MAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,UAAAC,QAAA;MACA,KAAAC,KAAA,CAAAT,IAAA,CAAAU,WAAA;IACA;IACAC,IAAA;MACA,KAAAZ,mBAAA;MACA,KAAAa,SAAA;QACA,KAAAL,SAAA;MACA;MACA;IACA;IACAM,KAAA;MACA,KAAAC,MAAA,CAAAC,IAAA,MAAAC,QAAA,sBAAAhB,IAAA,EAAAiB,IAAA,CAAAC,GAAA,IAAAA,GAAA,CAAA5B,IAAA,EAAA2B,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,IAAAA,GAAA,CAAAG,IAAA;UACA;UACA;UACA,KAAAC,QAAA;YACAC,OAAA;YACAC,IAAA;UACA;UACA,KAAAzB,mBAAA;UACA,KAAA0B,QAAA;QAEA;UACA,KAAAH,QAAA;YACAC,OAAA;YACAC,IAAA;UACA;QACA;MACA;IACA;IACAE,QAAA;MACA,KAAAZ,MAAA,CAAAa,GAAA,MAAAX,QAAA,iBAAAC,IAAA,CAAAC,GAAA,IAAAA,GAAA,CAAA5B,IAAA,EAAA2B,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA;IACA;IACAU,WAAA;MACA,KAAAvC,IAAA;MACA,KAAAM,GAAA;IACA;IACA8B,SAAA;MACA,KAAAX,MAAA,CAAAC,IAAA,MAAAC,QAAA;QACAvB,OAAA,OAAAA,OAAA;QACAD,QAAA,OAAAA,QAAA;QACAqC,KAAA;UACAxC,IAAA,OAAAA,IAAA;UACAM,GAAA,OAAAA;QACA;MACA,GAAAsB,IAAA,CAAAC,GAAA,IAAAA,GAAA,CAAA5B,IAAA,EAAA2B,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,IAAAA,GAAA,CAAAG,IAAA;UACA,KAAA9B,SAAA,GAAA2B,GAAA,CAAA5B,IAAA;UACA,KAAAI,KAAA,GAAAwB,GAAA,CAAAxB,KAAA;QACA;UACAoC,KAAA;QACA;MACA;IACA;IACAC,iBAAAC,GAAA;MACAb,OAAA,CAAAC,GAAA,OAAAY,GAAA;MACA,KAAAvC,OAAA;MACA,KAAAD,QAAA,GAAAwC,GAAA;MACA,KAAAP,QAAA;IACA;IACAQ,oBAAAD,GAAA;MACAb,OAAA,CAAAC,GAAA,SAAAY,GAAA;MACA,KAAAvC,OAAA,GAAAuC,GAAA;MACA,KAAAP,QAAA;IACA;EACA;EACAS,YAAA;IACA;IACA,KAAAT,QAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}